<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ares.seckill.mapper.TSeckillGoodsMapper">
    <update id = "updateSeckillStockByGoodsId">
        update ad_seckill.t_seckill_goods SET stock_count = #{stockCount} WHERE goods_id = #{goodsId}
    </update>
</mapper>
